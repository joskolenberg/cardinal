{
  "name": "joskolenberg/cardinal",
  "description": "Small wrapper for cardinal directions",
  "license": "MIT",
  "keywords": [
    "cardinal",
    "driections",
    "north",
    "east",
    "south",
    "west"
  ],
  "authors": [
    {
      "name": "Jos Kolenberg",
      "email": "jos@kolenberg.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "JosKolenberg\\Cardinal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JosKolenberg\\Cardinal\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.0|^8.0"
  }
}
